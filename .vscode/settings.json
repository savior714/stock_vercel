{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "java.import.gradle.enabled": false,
  "http.proxySupport": "fallback",
  "java.import.exclusions": [
    "**/node_modules/**"
  ],
  "java.configuration.updateBuildConfiguration": "automatic"
}